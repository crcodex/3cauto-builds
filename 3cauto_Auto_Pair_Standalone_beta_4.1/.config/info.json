{
    "3c_api":{
        "endpoint":"https://api.3commas.io",
        "version":"/public/api",
        "get":{
            "test":"/ver1/ping",
            "server_time":"/ver1/time",
            "bot_info":"/ver1/bots/{bot_id}/show",
            "deal_info":"/ver1/deals",
            "get_account":"/ver1/accounts",
            "get_deal":"/ver1/deals/{id}/show",
            "get_bot_stat":"/ver1/bots/stats",
            "get_bot":"/ver1/bots",
            "get_account_leverage_data":"/ver1/accounts/{account_id}/leverage_data",
            "get_single_account_or_summary":"/ver1/accounts/{account_id}",
            "get_balance_history":"/ver1/accounts/{account_id}/balance_chart_data",
            "get_active_trade":"/ver1/accounts/{account_id}/active_trading_entities",
            "get_info_to_add_fund":"/ver1/deals/{deal_id}/data_for_adding_funds"
        },
        "post":{
            "post_account_table_data":"/ver1/accounts/{account_id}/account_table_data",
            "post_disable_bot":"/ver1/bots/{bot_id}/disable",
            "post_enable_bot":"/ver1/bots/{bot_id}/enable",
            "post_panic_sell":"/ver1/deals/{deal_id}/panic_sell"
        },
        "patch":{
            "update_deal":"/ver1/deals/{deal_id}/update_deal",
            "update_bot":"/ver1/bots/{bot_id}/update"
        }
    },
    "bn_api":{
        "endpoint":"https://fapi.binance.com",
        "get":{
            "server_time":"/fapi/v1/time",
            "get_account_info":"/fapi/v2/account",
            "get_exchange_info":"https://api.binance.com/api/v3/exchangeInfo"
        }
    },
    "cg_api":{
        "endpoint":"https://api.coingecko.com/api/v3",
        "get":{
            "get_coin_market":"/coins/markets",
            "get_coin_list":"/coins/list",
            "get_coin_info":"/coins/{id}"
        }
    },
    "http":{
        "response":{
            "success":200,
            "post_success":201,
            "3c_limit_break":429
        }
    },
	"db":{
        "exchange_account" : "exchange_account",
        "deal" : "deal",
        "future_deal" : "future_deal",
        "bot" : "bot"
    }
}